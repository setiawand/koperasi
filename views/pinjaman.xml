<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="koperasi.list">     
      <field name="name">koperasi list</field>
      <field name="model">pinjaman</field>
      <field name="arch" type="xml">
        <tree>
          <field name="kode_pinjaman"/>
          <field name="nama_pinjaman"/>
          <field name="keterangan"/>
        </tree>
      </field>
    </record>


    <menuitem name="Pinjaman" id="koperasi.pinjaman" parent="koperasi.menu_root"/>
    <!-- <menuitem name="Menu 2" id="koperasi.menu_2" parent="koperasi.menu_root"/> -->

    <!-- actions -->
    <menuitem name="Jenis Pinjaman" id="koperasi.jenis_pinjaman" parent="koperasi.pinjaman"
              action="koperasi.action_window"/>
  </data>
</odoo>
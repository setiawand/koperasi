-->

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="koperasi.action_window">
      <field name="name">Data Anggota</field>
      <field name="res_model">anggota</field>
      <field name="view_mode">tree,form</field>
    </record>


